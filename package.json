{
  "dependencies": {
    "@types/node": "8.9.1",
    "neon-cli": "0.1.22"
  },
  "description": "Node.js bindings to rust_sodium.",
  "devDependencies": {
    "babel-eslint": "8.2.1",
    "cross-env": "5.1.3",
    "eslint": "4.17.0",
    "eslint-config-prettier": "2.9.0",
    "eslint-plugin-prettier": "2.6.0",
    "eslint-plugin-react": "7.6.1",
    "husky": "0.14.3",
    "jasmine": "3.0.0",
    "libsodium-wrappers-sumo": "0.7.3",
    "lint-staged": "6.1.0",
    "prettier": "1.10.2",
    "rimraf": "2.6.2",
    "typescript": "2.7.1"
  },
  "files": [
    "dist"
  ],
  "lint-staged": {
    "*.js": [
      "eslint --ignore-path .prettierignore --fix",
      "git add"
    ]
  },
  "license": "GPL-3.0",
  "main": "dist/index.js",
  "name": "libsodium-neon",
  "repository": {
    "type": "git",
    "url": "https://github.com/wireapp/libsodium-neon.git"
  },
  "scripts": {
    "build": "cross-env RUST_SODIUM_DISABLE_PIE=1 neon build",
    "clear": "rimraf dist",
    "dist": "yarn clear && tsc && yarn build",
    "fix": "yarn test:jsScript --fix",
    "precommit": "lint-staged",
    "test": "yarn dist && yarn test:node",
    "test:jsScript": "eslint --ignore-path .gitignore --ignore-path .prettierignore **/*.{js,ts}",
    "test:node": "jasmine --config=test/support/jasmine.json"
  },
  "types": "dist/index.d.ts",
  "version": "2.1.1"
}
