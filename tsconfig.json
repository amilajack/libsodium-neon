{
  "compilerOptions": {
    "declaration": true,
    "lib": ["DOM", "ES6"],
    "module": "CommonJS",
    "moduleResolution": "node",
    "noEmitOnError": true,
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "outDir": "dist",
    "removeComments": true,
    "rootDir": "src",
    "sourceMap": false,
    "strict": true,
    "target": "ES5"
  },
  "exclude": ["node_modules", "dist"],
}
